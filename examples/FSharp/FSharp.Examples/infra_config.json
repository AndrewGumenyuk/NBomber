{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Debug",
    "Enrich": ["FromLogContext", "WithMachineName"],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] [{MachineName}] [{SessionId}/{TestName}] {NewLine}{Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}